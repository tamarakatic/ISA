<table class="table">
  <thead>
    <tr>
      <th>Item</th>
      <th>Quantity</th>
      <th>Unit</th>
    </tr>
  </thead>
  <tbody>
    <% order.order_items.each do |item| %>
    <tr id="<%= item.id %>">
      <td>
        <%= item.item %>
      </td>
      <td>
        <%= item.quantity %>
      </td>
      <td>
        <%= item.unit_of_measure %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
